  "conflict": {
    "drupal/drupal": "*"
  },
  "scripts": {
    "arocom-base-patches": "DrupalProject\\composer\\ScriptHandler::applyBasePatches",
    "arocom-post-scaffold": "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
    "arocom-git-purge": "DrupalProject\\composer\\ScriptHandler::removeGitRepositories",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::postInstall"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::postUpdate"
    ],
    "tdd-phantomjs": [
      "mkdir -p drupal/sites/default/simpletest && chmod 777 drupal/sites/default/simpletest",
      "./tests/bin/phantomjs --ssl-protocol=any --ignore-ssl-errors=true ./vendor/jcalderonzumba/gastonjs/src/Client/main.js 8510 1024 768 > /dev/null 2>&1"
    ],
    "tdd-phpunit": [
      "./vendor/bin/phpunit -c core/phpunit.xml --testsuite=unit"
    ],
    "aro-sett": [
      "sed -i -e 322,330's/.*/#&/' ./scripts/composer/ScriptHandler.php"
    ],
    "aro-sett1": [
      "sed -i -e 322,330's/#//' ./scripts/composer/ScriptHandler.php"
    ],
    "aro-sett2": [
      "sed -i -e '168d' ./settings/default/settings.local.php"
    ]
  },
  "scripts-descriptions": {
    "tdd-phantomjs": "Drupal 8: Run PhantomJS.",
    "tdd-phpunit": "Drupal 8: Run Drupal's 8 core Unit tests."
  },
  "extra": {
    "installer-types": [
      "npm-asset",
      "bower-asset",
      "arocom-module",
      "arocom-theme",
      "arocom-profile",
      "arocom-drush"
    ],
    "installer-paths": {
      "drupal/core": [
        "type:drupal-core"
      ],
      "drupal/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "drupal/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "drupal/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "drupal/modules/arocom/{$name}": [
        "type:arocom-module"
      ],
      "drupal/profiles/arocom/{$name}": [
        "type:arocom-profile"
      ],
      "drupal/themes/arocom/{$name}": [
        "type:arocom-theme"
      ],
      "drush/arocom/{$name}": [
        "type:arocom-drush"
      ],
      "drupal/libraries/{$name}": [
        "type:npm-asset",
        "type:bower-asset",
        "type:drupal-library",
        "type:arocom-library"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "merge-plugin": {
      "include": [
        "drupal/modules/custom/**/*/composer.json",
        "drupal/modules/custom/*/composer.json",
        "drupal/modules/arocom/**/*/composer.json",
        "drupal/modules/arocom/*/composer.json",
        "drupal/profiles/custom/*/composer.json",
        "drupal/profiles/arocom/*/composer.json",
        "drupal/themes/custom/*/composer.json",
        "drupal/themes/arocom/*/composer.json"
      ]
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "./drupal",
        "project-root": "./drupal"
      },
      "file-mapping": {
        "[web-root]/sites/default/default.settings.php.orig": false,
        "[web-root]/sites/default/default.settings.php": false,
        "[web-root]/sites/example.settings.local.php": false,
        "[web-root]/../settings/default/settings.php": {
          "append": "./drupal/sites/default/default.settings.php",
          "force-append": false
        }
      }
    }
  }
}
